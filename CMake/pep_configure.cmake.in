
set (environment "@process_environment@")
set (command @step_command@)

set (key)
foreach (arg IN ITEMS ${environment})
  if (NOT key)
    set (key "${arg}")
  else ()
    # message ("setting ${key}=${arg}")
    set (ENV{${key}} ${arg})
    set (key)
  endif()
endforeach()

execute_process(COMMAND ${command}
  RESULT_VARIABLE rv)

if (NOT "${rv}" STREQUAL "0")
  message(FATAL_ERROR "Failed!!!")
endif()
